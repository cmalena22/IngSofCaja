<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">

<navigation-rule>
<from-view-id>/Login.xhtml</from-view-id>
<navigation-case>
<from-outcome>exito</from-outcome>
<to-view-id>/ParteAdministrador.xhtml</to-view-id>
</navigation-case>
<navigation-case>
<from-outcome>fallo</from-outcome>
<to-view-id>/Login.xhtml</to-view-id>
</navigation-case>
</navigation-rule>

<navigation-rule>
	<display-name>ParteAdministrador.xhtml</display-name>
	<from-view-id>/ParteAdministrador.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>ingreSocio</from-outcome>
		<to-view-id>/ingresarUsuario.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>

<navigation-rule>
	<display-name>ParteAdministrador.xhtml</display-name>
	<from-view-id>/ParteAdministrador.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>ingreLibrodiario</from-outcome>
		<to-view-id>/LibroDiario.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>
</faces-config>
